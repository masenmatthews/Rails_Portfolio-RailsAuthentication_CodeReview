<%= render "layouts/errors", :object => @post %>

<div class="card-content cyan lighten-1 white-text z-depth-3">
  <%= form_for @post, url: posts_path, html: { multipart: true } do |f| %>
    <%= f.label :post_title %>
    <%= f.text_field :post_title %>
    <br>
    <%= f.label :post_body %>
    <%= f.text_field :post_body %>
    <br>
    <%= f.file_field :image %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
