<div class="card-content blue-grey white-text z-depth-3">
  <h2 class="title-head">Masen's Portfolio</h1>
</div>

<div class="card-content blue-grey white-text z-depth-3">
<p><%= link_to "New post", new_post_path %></p>

  <% if @posts.any? %>
    <ul>
      <% @posts.each do |post| %>
      <li class="blog-title"><%= link_to post.post_title, post_path(post) %></li>
      <% end %>
    </ul>
    <% else %>
    <p>There are no posts yet!</p>
    <% end %>
</div>
