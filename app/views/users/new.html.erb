<div class="well">
  <h1> Sign up</h1>
    <%= form_for @user do |f| %>
    <%= f.label "User Name" %>
    <%= f.text_field :username %>
    <br>
    <%= f.label "Email" %>
    <%= f.text_field :email %>
    <br>
    <%= f.label "Password" %>
    <%= f.password_field :password %>
    <br>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
    <br>
    <%= f.submit "Sign Up" %>
  <% end %>
</div>
